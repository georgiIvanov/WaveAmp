CLANG_CXX_LANGUAGE_STANDARD = c++11
CLANG_CXX_LIBRARY = libc++
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/BFPaperCheckbox" "${PODS_ROOT}/Headers/Public/EZAudio" "${PODS_ROOT}/Headers/Public/NVDSP" "${PODS_ROOT}/Headers/Public/Novocaine" "${PODS_ROOT}/Headers/Public/TPCircularBuffer" "${PODS_ROOT}/Headers/Public/UIColor+BFPaperColors" "${PODS_ROOT}/Headers/Public/pop"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/BFPaperCheckbox" -isystem "${PODS_ROOT}/Headers/Public/EZAudio" -isystem "${PODS_ROOT}/Headers/Public/NVDSP" -isystem "${PODS_ROOT}/Headers/Public/Novocaine" -isystem "${PODS_ROOT}/Headers/Public/TPCircularBuffer" -isystem "${PODS_ROOT}/Headers/Public/UIColor+BFPaperColors" -isystem "${PODS_ROOT}/Headers/Public/pop"
OTHER_LDFLAGS = $(inherited) -ObjC -l"Pods-BFPaperCheckbox" -l"Pods-EZAudio" -l"Pods-NVDSP" -l"Pods-Novocaine" -l"Pods-TPCircularBuffer" -l"Pods-UIColor+BFPaperColors" -l"Pods-pop" -l"c++" -framework "AVFoundation" -framework "Accelerate" -framework "AudioToolbox" -framework "GLKit"
OTHER_LIBTOOLFLAGS = $(OTHER_LDFLAGS)
PODS_ROOT = ${SRCROOT}/Pods